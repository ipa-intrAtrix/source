<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="ipa_intrAtrix.Models.Contracts" assembly="ipa_intrAtrix">

  <class name="Waranty" table="Garantie" lazy="false">

    <id name="WarantyId">
      <column name="gar_id" sql-type="integer" not-null="true"/>
      <generator class="native" />
    </id>

    <property name="WarantyDescr">
      <column name="gar_beschreibung" length="35" not-null="true" />
    </property>

  </class>

</hibernate-mapping>